<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="s_cron_sale_order_lazada_error" forcecreate='True' model="ir.cron">
            <field name="name">###Queue Lazada sale order error</field>
            <field name="active">True</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_s_lazada_queue"/>
            <field name="state">code</field>
            <field name="code">model.cron_sale_order_lazada_error()</field>
        </record>

        <record id="cron_create_sale_order_marketplace" forcecreate='True' model="ir.cron">
            <field name="name">###Marketplace: Cron create sale order</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_s_mkp_order_queue"/>
            <field name="state">code</field>
            <field name="code">model.s_cronjob_create_data_queue()</field>
            <field eval="True" name="doall"/>
        </record>

        <record id="cron_s_mkp_resync_product" forcecreate='True' model="ir.cron">
            <field name="name">###Marketplace: Cron resync product</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_s_mkp_resync_product"/>
            <field name="state">code</field>
            <field name="code">model.cronjob_resync_product()</field>
            <field eval="True" name="doall"/>
        </record>
    </data>
</odoo>
