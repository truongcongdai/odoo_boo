<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="ir_cron_data_refresh_access_token" model="ir.cron">
            <field name="name">### Lazada Cron Refresh access token </field>
            <field name="model_id" ref="base.model_res_config_settings"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cronjob_refresh_token()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="numbercall">-1</field>
            <field name="doall">1</field>
            <field name="nextcall" eval="datetime.now() + timedelta(days=1)"/>
        </record>

