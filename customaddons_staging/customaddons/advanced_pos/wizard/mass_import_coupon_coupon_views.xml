<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mass_import_coupon_coupon_views_form" model="ir.ui.view">
        <field name="name">mass.import.coupon.coupon.view.form</field>
        <field name="model">mass.import.coupon.coupon</field>
        <field name="arch" type="xml">
            <form string="Import">
                <sheet>
                    <field name="s_import_coupon_coupon_ids" readonly="1"/>
                </sheet>
                <footer>
                    <button name="action_import_coupon" type="object"
                            string="Import" class="btn-primary"/>
                    <button special="cancel" string="Cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="mass_import_coupon_coupon_wizard_action" model="ir.actions.act_window">
        <field name="name">Import</field>
        <field name="res_model">mass.import.coupon.coupon</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="context">{'default_s_import_coupon_coupon_ids': active_ids}</field>
        <field name="binding_model_id" ref="model_s_import_coupon_coupon"/>
    </record>

</odoo>

