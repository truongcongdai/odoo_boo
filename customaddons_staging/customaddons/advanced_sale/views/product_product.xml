<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="s_product_product_inherit" model="ir.ui.view">
            <field name="name">s_product_product_inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="odoo_multi_channel_sale.product_normal_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='wk_default_code']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>