<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="s_product_gender_tree_view" model="ir.ui.view">
        <field name="name">s.product.gender.tree</field>
        <field name="model">s.product.gender</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>
    <record id="s_product_gender_act_window" model="ir.actions.act_window">
        <field name="name">Giới tính</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">s.product.gender</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem name="#Giới tính" id="s_product_gender_menu" parent="advanced_inventory.s_product_information_menu" action="s_product_gender_act_window" sequence="10"/>
</odoo>
