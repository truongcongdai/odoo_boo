<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="s_stock_picking_cron" forcecreate='True' model="ir.cron">
            <field name="name">###Thông báo quá hạn phiếu điều chuyển</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_stock_picking"/>
            <field name="state">code</field>
            <field name="code">model._cron_notification_deadline()</field>
        </record>
        <record forcecreate="True" id="ir_cron_force_on_cron_bravo" model="ir.cron">
            <field name="name">###Cronjob Cảnh báo sản phẩm</field>
            <field name="model_id" ref="model_stock_quant"/>
            <field name="state">code</field>
            <field name="code">model._cron_compute_time_quantity_warning()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>