<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" id="ir_cron_force_on_cron_bravo" model="ir.cron">
            <field name="name">### Bravo - Force enable/unable Bravo cronjob</field>
            <field name="model_id" ref="pos_sale.model_pos_order"/>
            <field name="state">code</field>
            <field name="code">model.cron_force_on_off_bravo_cron()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>

        <record id="ir_cron_get_token_bravo" model="ir.cron">
            <field name="name">### Bravo: Automated Check Expires In Token Bravo</field>
            <field name="doall" eval="True"/>
            <field name="priority" eval="True"/>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="interval_type">minutes</field>
            <field name="interval_number">1</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="model_id" ref="model_base_integrate_bravo"/>
            <field name="code">model.cron_get_token_bravo()</field>
        </record>

        <record forcecreate="True" id="ir_cron_sync_bravo_product" model="ir.cron">
            <field name="name">### Bravo: Automated Synchronize Products</field>
            <field name="model_id" ref="base.model_res_company"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_products()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/>
        </record>


        <record forcecreate="True" id="ir_cron_post_retail_sale_bravo" model="ir.cron">
            <field name="name">### Bravo - CreateRetail: Bảng kê chi tiết bán lẻ</field>
            <field name="model_id" ref="pos_sale.model_pos_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_post_bravo_retail_sale()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall"
                   eval="(DateTime.now().replace(hour=17, minute=0, second=0)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="active" eval="False"/>
        </record>


