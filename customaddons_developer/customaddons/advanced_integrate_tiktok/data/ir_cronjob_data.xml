        <record id="cronjob_refresh_token_tiktok" forcecreate='True' model="ir.cron">
            <field name="name">###Tiktok Integrate - Refresh Token</field>
            <field eval="True" name="active"/>
            <field name="priority">1</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_res_config_settings"/>
            <field name="state">code</field>
            <field name="code">model.refresh_token()</field>
            <field eval="True" name="doall"/>
        </record>

